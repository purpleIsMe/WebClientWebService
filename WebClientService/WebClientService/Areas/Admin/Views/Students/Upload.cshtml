@model Model.EF.Student
@using Model.DAO
@using System
@using System.Collections.Generic
@using Model.EF

@{
    List<Student> listu = ViewBag.ListStu;
    SelectList userList = ViewBag.ID;
    SelectList users = ViewBag.user;
}
@{
    ViewBag.Title = "Upload";
    Layout = "/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@section Header
{
    DANH SÁCH SINH VIÊN
}
@section js
{
    <script src="~/Assets/Admin/js/student.js"></script>
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                DANH SÁCH NGƯỜI DÙNG
            </div>
            <div>
                <div class="form-group" id="dlLecturer">
                    <div class="col-md-offset-2 col-md-10">
                        <div class="control-label col-md-2">Chọn giảng viên</div>
                        <div class="col-md-3">
                            <select id="lecturer" class="form-control"></select>
                        </div>
                        <div class="control-label col-md-2">Chọn lớp</div>
                        <div class="col-md-3">
                            <select id="class" class="form-control"></select>
                        </div>
                    </div>
                </div>
            @using (Html.BeginForm("Upload", "Students", FormMethod.Post, new { enctype = "multipart/form-data", id = "form1" }))
            {
                @Html.ValidationSummary(true)
                <div class="form-horizontal">
                    <div class="form-group">
                        <br /><br />
                        <div class="control-label col-md-2">Excel:</div>
                        <div class="col-md-10">
                            <input type="file" id="FileUpload" name="FileUpload" class="" />
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            <input type="submit" value="Upload" id="btnSubmit" class="btn btn-primary" />
                        </div>
                    </div>

                </div>
                @Html.ValidationMessage("")
            }
                <div id="pleaseWaitDialog"></div>
            </div>
        </div>
    </div>
</div>